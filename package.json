{
    "name": "mini-program-schema",
    "version": "1.0.0",
    "main": "index.js",
    "repository": "https://github.com/cwxyz007/mini-program-schema.git",
    "author": "chuisi-wangjie <chuisi.wj@raycloud.com>",
    "license": "MIT",
    "scripts": {
        "ali:component": "typescript-json-schema src/ali.component.d.ts IOption -o build/ali.component.schema.json",
        "ali:page": "typescript-json-schema src/ali.page.d.ts IOption -o build/ali.page.schema.json",
        "wx:component": "typescript-json-schema src/wx.component.d.ts IOption -o build/wx.component.schema.json",
        "wx:page": "typescript-json-schema src/wx.page.d.ts IOption -o build/wx.page.schema.json",
        "ali": "yarn ali:component && yarn ali:page",
        "wx": "yarn wx:component && yarn wx:page",
        "build": "yarn ali:component && yarn ali:page && yarn wx:component && yarn wx:page"
    },
    "dependencies": {
        "typescript-json-schema": "^0.42.0"
    }
}
